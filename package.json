    "concurrently": "^4.1.2",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "if-env": "^1.0.4",
    "jwks-rsa": "^1.8.0",
    "mongo": "^0.1.0",
    "mongoose": "^5.9.9",
    "react-beautiful-dnd": "^13.0.0",
    "react-calendar": "^3.0.1",
    "socket.io": "^2.3.0"
  }
